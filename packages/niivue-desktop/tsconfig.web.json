{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": [
    "src/renderer/src/env.d.ts",
    "src/renderer/src/**/*",
    "src/renderer/src/**/*.tsx",
    "src/preload/*.d.ts",
    "src/common/**/*",
    "src/types",
  ],
  "compilerOptions": {
    "composite": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "types": ["vite/client"],
    "module": "NodeNext",
    "moduleResolution": "nodenext",
    "paths": {
      "@renderer/*": [
        "src/renderer/src/*",
        
      ],
      "@niivue/dicom-loader": ["../../dicom-loader/dist/index.d.mts"]
    }
  }
}
